# Sepolia Testnet RPC URL
SEPOLIA_RPC=https://rpc.sepolia.org

# BNB Smart Chain Testnet RPC URL
BSC_TESTNET_RPC=https://data-seed-prebsc-1-s1.binance.org:8545/

# Your wallet private key (NEVER commit the real .env file!)
# Export from MetaMask: Account Details > Export Private Key
DEPLOYER_PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Etherscan API Key (for Sepolia verification)
# Get from: https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# BSCScan API Key (for BSC verification)
# Get from: https://bscscan.com/myapikey
BSCSCAN_API_KEY=your_bscscan_api_key_here

# Initial supply of tokens to mint (in whole tokens, will be multiplied by 10^18)
INITIAL_SUPPLY=1000000
